mixin prod-item-sls(x,t,p)
	+b.item-prod--js
			+e.inner--js
				+e.head
					+e.status-wrap
						block
					+e.like
						+icon('star')
					+e.compare.active
						+icon('compare')
					+e.btn-fast.popup-with-move-anim(href="#modal-prod") Быстрый просмотр
					+e.img-wrap(href="#")
						span: img.res-img(src="img/prod-1.png" data-lazy=`img/prod-${x}.png`, alt="")
				+e.caption
					+e.title--js.h2(href="#")= t
					+e.text--js.small.text-gray= p
					+e.toggle-block
						+b.size-block
							.small Доступные размеры:
							+e.btn-wrap
								mixin in-ch( n, t)
									+b.LABEL.btn-input(data-toggle="popover" data-trigger="hover" data-content='<div class="pb-1"><span class="text-gray">Обхват головы: </span> <strong class="display-inline-block">24-28 см </strong></div><div class="pb-1"><span class="text-gray">Рекомендовано:  </span> <strong class="display-inline-block">новорожденным, недоношенным</strong></div>')
										+e.INPUT.input(type="radio" name= n)
										+e.SPAN.text= t
								+in-ch("size-prod","XL")
								+in-ch("size-prod","XL/L")
								+in-ch("size-prod","L")
								+in-ch("size-prod","L/M")
								+in-ch("size-prod","M")
								+in-ch("size-prod","M/S")
								+in-ch("size-prod","S")
								+in-ch("size-prod","S/XS")
								+in-ch("size-prod","XS")
								+in-ch("size-prod","Inf I")
								+in-ch("size-prod","Inf II")
								+in-ch("size-prod","Inf III")
					.row
						.col-12.d-none.d-sm-block
							// добавил 24.02
							.small.pb-1.text-primary  В наличии
							.small.pb-1.text-danger  Под заказ
							// /добавил 24.02
						.col-6.col-md-auto
							.h5.text-primary  15 000 Р
						.col-6.col-md-auto
							// добавил 24.02
							+e.btn-wrap.d-none.d-sm-block
								.smallest.text-danger.text-center  Выберите размер
								+e.A.btn.popup-with-move-anim(href="#modal-prod")  В корзину
							+e.btn-wrap.d-sm-none
								+e.A.btn.popup-with-move-anim.pl-0.pr-0(href="#modal-prod")
									+icon("basket")
										| Корзина
	// .item-prod


