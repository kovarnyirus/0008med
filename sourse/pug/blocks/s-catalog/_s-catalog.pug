mixin s-catalog()
	// start s-catalog
	+b.s-catalog.section#s-catalog
		.container
			+b.section-title
				.row
					.col
						.h2.text-gray.fw-900 РЕКОМЕНДУЕМЫЕ ТОВАРЫ
					.col-auto.d-none.d-md-block
						a.small(href="#") Смотреть все

			+e.slider--mobile
				- for (var x = 0; x < 2; x++)
